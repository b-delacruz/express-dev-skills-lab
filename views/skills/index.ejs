<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Skills</title>
</head>
<body>
  <h1>Skills</h1>

  <a href="/skills/new">Create new skills</a>
    <ul>
      <% skills.forEach(skills => { %>
        <li> 
          <%- skills.name %>
        </li>
        <% }) %>  
    </ul>

    <ul>
      <% skills.forEach(function(skill) { %> 
        <li>
          <a href="/skills/<%= skill._id %>"><%= skill.text %></a>
        </li>
      <% }) %> 
    </ul>    
</body>
</html>